{
  "name": "nice-speed-dials-chrome",
  "version": "4.0.0",
  "license": "MIT",
  "keywords": [
    "chrome",
    "extension",
    "speed dials",
    "solid",
    "solidjs"
  ],
  "author": {
    "name": "Krushang Kasundra",
    "url": "https://kruz.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kruzkasu223/nice-speed-dials-chrome"
  },
  "description": "Nice and Simple Speed Dials Extension for Chrome",
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "start": "vite",
    "dev": "vite build --mode development",
    "dev:build": "yarn dev && onchange -k './src/**' -- yarn dev",
    "zip": "bestzip extension.zip dist",
    "build": "vite build",
    "export": "rm -rf ./dist ./extension.zip && yarn build && yarn zip"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.23",
    "@pandacss/dev": "0.26.2",
    "@park-ui/panda-preset": "0.28.0",
    "@types/chrome": "0.0.254",
    "bestzip": "2.2.1",
    "onchange": "7.1.0",
    "sass": "1.69.5",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vite-plugin-solid": "2.8.0",
    "vite-tsconfig-paths": "4.2.3"
  },
  "dependencies": {
    "@ark-ui/solid": "1.2.0",
    "lucide-solid": "0.302.0",
    "solid-dnd-directive": "0.2.0",
    "solid-js": "1.8.7"
  }
}
