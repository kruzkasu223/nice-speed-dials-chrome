{
  "name": "nice-speed-dials-chrome",
  "version": "4.0.0",
  "license": "MIT",
  "keywords": [
    "chrome",
    "extension",
    "speed dials",
    "solid",
    "solidjs"
  ],
  "author": {
    "name": "Krushang Kasundra",
    "url": "https://kruz.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kruzkasu223/nice-speed-dials-chrome"
  },
  "description": "Nice and Simple Speed Dials Extension for Chrome",
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "start": "vite",
    "dev": "vite build --mode development",
    "dev:build": "yarn dev && onchange -k './src/**' -- yarn dev",
    "build": "vite build",
    "zip": "cd dist/ && bestzip ../extension.zip * .vite",
    "export": "rm -rf ./dist ./extension.zip && yarn build && yarn zip",
    "parkui:add": "npx @park-ui/cli components add",
    "parkui:update": "npx @park-ui/cli components add button spinner dialog drawer icon-button input menu text toast tooltip accordion switch slider color-picker file-upload number-input radio-button-group",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.2.0",
    "@pandacss/dev": "1.4.3",
    "@park-ui/panda-preset": "0.43.1",
    "@types/chrome": "0.1.24",
    "bestzip": "2.2.1",
    "onchange": "7.1.0",
    "prettier": "3.6.2",
    "sass": "1.93.2",
    "typescript": "5.9.3",
    "vite": "7.1.11",
    "vite-plugin-solid": "2.11.9",
    "vite-plugin-zip-pack": "1.2.4",
    "vite-tsconfig-paths": "5.1.4"
  },
  "dependencies": {
    "@ark-ui/solid": "5.26.2",
    "caniuse-lite": "1.0.30001751",
    "lucide-solid": "0.546.0",
    "solid-dnd-directive": "0.2.0",
    "solid-js": "1.9.9"
  }
}
