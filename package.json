{
  "name": "nice-speed-dials-extension",
  "version": "5.0.0",
  "license": "MIT",
  "keywords": [
    "browser",
    "extension",
    "speed dials",
    "solid",
    "solidjs"
  ],
  "author": {
    "name": "Krushang Kasundra",
    "url": "https://kruz.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kruzkasu223/nice-speed-dials-extension"
  },
  "description": "Nice and Simple Speed Dials Extension for Browser",
  "type": "module",
  "scripts": {
    "prepare": "panda codegen && wxt prepare",
    "dev": "wxt --mv3",
    "dev:firefox": "wxt -b firefox --mv3",
    "build": "wxt build --mv3",
    "build:firefox": "wxt build -b firefox --mv3",
    "zip": "wxt zip --mv3 && cp .output/extension-chrome.zip extension-chrome.zip",
    "zip:firefox": "wxt zip -b firefox --mv3 && cp .output/extension-firefox.zip extension-firefox.zip",
    "zip:edge": "wxt zip -b edge --mv3 && cp .output/extension-edge.zip extension-edge.zip",
    "parkui:add": "npx @park-ui/cli components add",
    "parkui:update": "npx @park-ui/cli components add button alert spinner dialog drawer icon-button input menu text toast tooltip accordion switch slider color-picker file-upload number-input radio-button-group",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@pandacss/dev": "1.4.3",
    "@park-ui/panda-preset": "0.43.1",
    "@types/chrome": "0.1.24",
    "@wxt-dev/module-solid": "1.1.4",
    "prettier": "3.6.2",
    "sass-embedded": "1.93.2",
    "typescript": "5.9.3",
    "vite": "7.1.11",
    "vite-plugin-solid": "2.11.9",
    "wxt": "0.20.11"
  },
  "dependencies": {
    "@ark-ui/solid": "5.26.2",
    "caniuse-lite": "1.0.30001751",
    "lucide-solid": "0.546.0",
    "solid-dnd-directive": "0.2.0",
    "solid-js": "1.9.9"
  }
}
