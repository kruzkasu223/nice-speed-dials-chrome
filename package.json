{
  "name": "nice-speed-dials-chrome",
  "version": "4.0.0",
  "license": "MIT",
  "keywords": [
    "chrome",
    "extension",
    "speed dials",
    "solid",
    "solidjs"
  ],
  "author": {
    "name": "Krushang Kasundra",
    "url": "https://kruz.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kruzkasu223/nice-speed-dials-chrome"
  },
  "description": "Nice and Simple Speed Dials Extension for Chrome",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite build --mode development",
    "dev:build": "yarn dev && onchange -k './src/**' -- yarn dev",
    "zip": "bestzip extension.zip dist",
    "build": "vite build",
    "export": "rm -rf ./dist ./extension.zip && yarn build && yarn zip"
  },
  "devDependencies": {
    "@types/chrome": "0.0.254",
    "bestzip": "2.2.1",
    "onchange": "7.1.0",
    "sass": "^1.69.5",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vite-plugin-solid": "2.8.0"
  },
  "dependencies": {
    "@hope-ui/core": "1.0.0-next.11",
    "@hope-ui/solid": "0.6.7",
    "@stitches/core": "1.2.8",
    "clsx": "2.0.0",
    "lucide-solid": "0.302.0",
    "solid-dnd-directive": "0.2.0",
    "solid-js": "1.8.7",
    "solid-toast": "0.5.0",
    "solid-transition-group": "0.2.3"
  }
}
